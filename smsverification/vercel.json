{
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "https://mysmsnumber.onrender.com/:path*" 
    },
    {
      "source": "/ws/:path*",
      "destination": "https://mysmsnumber.onrender.com/ws/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
